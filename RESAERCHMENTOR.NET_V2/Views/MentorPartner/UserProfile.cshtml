                                                        </div><!-- /news-feed-text -->
                                                    </div><!-- /write-up -->
                                                    <!--/div-->

                                                </div><!-- /row -->
                                            </div>
                                                 }
                                            }
                                            <!-- /story-cont -->
                                        </div>
                                    </div>
                                </div>
