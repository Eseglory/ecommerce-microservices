                                                        <div class="row">
                                                           @if (item.ProfilePicsName == string.Empty && item.Gender == "Male")
                                                           {
                                                            <img ID="PictureImageA" class="img-responsive" src="~/Content/dist/img/userMale.png" />
                                                           }

                                                           else if (item.ProfilePicsName == string.Empty && item.Gender == "Female")
                                                           {
                                                            <img ID="PictureImageA" class="img-responsive" src="~/Content/dist/img/userfeMale.png" />
