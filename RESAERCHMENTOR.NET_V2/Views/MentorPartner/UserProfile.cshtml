                                <div class="col-md-12">
                                    <div class="pt-20">
                                        <div class="streamline user-activity">
                                            @{
                                                foreach (var act in Model.MyActivities)
                                                {
                                                    /*DateTime DT = Convert.ToDateTime(act.ActivityDateCreated)*/
                                                    ;
                                                    var TStamp = act.ActivityDateCreated;//DT.ToLongTimeString();
