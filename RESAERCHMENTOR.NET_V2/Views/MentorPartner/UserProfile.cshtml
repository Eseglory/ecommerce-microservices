@model RESAERCHMENTOR.NET_V2.Models.MyModelObjects
@using Microsoft.AspNet.Identity
@using System.Globalization
@using ModelBinderAttribute = System.Web.Http.ModelBinding.ModelBinderAttribute
@{
    ViewBag.Title = "UserProfile";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}



<!-- Title -->
<div class="row heading-bg">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h5 class="txt-dark">Profile</h5>
        <dv align="center">
            <span style="color:green"><strong>@ViewBag.Message</strong></span>
        </dv>
    </div>
    <!-- Breadcrumb -->
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li class="active"><span>profile</span></li>
        </ol>
    </div>
    <!-- /Breadcrumb -->
</div>
<!-- /Title -->

<!-- Row -->
<div class="row">
    <div class="col-lg-3 col-xs-12">
        <div class="panel panel-default card-view  pa-0">
            <div class="panel-wrapper collapse in">
                <div class="panel-body  pa-0">
                    <div class="profile-box">
                        <div class="profile-cover-pic">
                            <div class="profile-image-overlay"></div>
                        </div>
                        <div class="profile-info text-center">
                            <div class="profile-img-wrap">
                                @if (@Model.MyProfile.ProfilePicsName == string.Empty && Model.MyProfile.Gender == "Male")
                                {
                                    <img src="~/Content/dist/img/userMale.png" alt="user_auth" class="user-auth-img img-circle" width="137" height="137" />
                                }
                                else if (@Model.MyProfile.ProfilePicsName == string.Empty && Model.MyProfile.Gender == "Female")
                                {
                                    <img src="~/Content/dist/img/userfeMale.png" alt="user_auth" class="user-auth-img img-circle" width="137" height="137" />
                                }
                                else
                                {
                                    <img id = "Image2" class="inline-block mb-10 user-auth-img img-circle" src="~/Files/@Model.MyProfile.ProfilePicsName" width="137" height="137" />
                                }

                            </div>
                            <h5 class="block mt-10 mb-5 weight-500 capitalize-font txt-info">
                                @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                                {
                                    @Html.AntiForgeryToken()
                                    if (Model.MyProfile.FullName == string.Empty)
                                    {
                                        @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
                                    }
                                    else
                                    {
                                    @Html.ActionLink("Hello " + Model.MyProfile.FullName + "", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })
                                    }
                                }
                            </h5>
                            <h6 class="block capitalize-font pb-20"></h6>
                        </div>
                        <div class="social-info">
                            <div class="row">
                                <div class="col-xs-4 text-center">
                                    <span class="counts block head-font"><span class="counter-anim">@Model.MyResearch.Count()</span></span>
                                    <span class="counts-text block">Posts</span>
                                </div>
                                <div class="col-xs-4 text-center">
                                    <span class="counts block head-font"><span class="counter-anim">@Model.FollowCount</span></span>
                                    <span class="counts-text block">Followers</span>
                                </div>
                                <div class="col-xs-4 text-center">
                                    <span class="counts block head-font"><span class="counter-anim">@Model.FollowingCount</span></span>
                                    <span class="counts-text block">Following</span>
                                </div>
                            </div>
                            <button class="btn btn-info btn-block  btn-anim mt-30" data-toggle="modal" data-target="#myModal"><i class="fa fa-pencil"></i><span class="btn-text">edit profile</span></button>
                            <a href="@Url.Action("UserPayment","MentorPartner")" class="btn btn-info btn-block  btn-anim mt-30" data-toggle="modal"  title="Pay For Peer Review" ><i class="fa fa-money"></i><span class="btn-text">Payment</span></a>
                            <div id="myModal" class="modal fade in" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
