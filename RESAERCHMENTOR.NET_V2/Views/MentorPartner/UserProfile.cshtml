                                                                                        <div class="input-group-addon"><i class="icon-envelope-open"></i></div>
                                                                                        <input type="email" class="form-control" name="OwnersId" required="required" readonly="readonly" id="OwnersId" placeholder="xyz@gmail.com" value=@User.Identity.GetUserName()>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputContact_1">Contact number</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-phone"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.CNumber, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputContact_1">Date Of Birth number</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-phone"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.BDate, new { @class = "form-control", type = "date", required = true })
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Gender</label>
                                                                                    <div>
                                                                                        <div class="radio">
                                                                                            @Html.RadioButtonFor(m => m.MyProfile.Gender, "Male")
                                                                                            <label for="Gender">
                                                                                                Male
                                                                                            </label>
                                                                                        </div>
                                                                                        <div class="radio">
                                                                                            @Html.RadioButtonFor(m => m.MyProfile.Gender, "Female")
                                                                                            <label for="Gender">
                                                                                                Female
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Who are you?</label>
                                                                                    @Html.DropDownListFor(m => m.MyProfile.WhoYouAre, new SelectList(Model.MyProfile.WhoYouAreList, "Name", "Name"), "Select Item", new { @class = "form-control" })
                                                                                    @Html.ValidationMessageFor(m => m.MyProfile.WhoYouAre)
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Institution</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>

                                                                                        @Html.TextBoxFor(m => m.MyProfile.Institution, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Qualification</label>

                                                                                    @Html.TextBoxFor(m => m.MyProfile.Qualification, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Primary Field of Expertise</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.DropDownListFor(m => m.MyProfile.Expertise, new SelectList(Model.MyProfile.ExpertiseList, "Name", "Name"), "Select Expertise", new { @class = "form-control" })
                                                                                        @Html.ValidationMessageFor(m => m.MyProfile.Expertise)
                                                                                    </div><br /><br />

                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Willing to be</label>
                                                                                    @Html.DropDownListFor(m => m.MyProfile.WillingToBe, new SelectList(Model.MyProfile.Willing, "Name", "Name"), "Select Item", new { @class = "form-control" })
                                                                                    @Html.ValidationMessageFor(m => m.MyProfile.WillingToBe)
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Mark areas you are willing to mentor  or provide academic services </label>
                                                                                    @if (@Model.MyProfile.MentorCategory != string.Empty)
                                                                                    {
                                                                                        @Html.TextBoxFor(m => m.MyProfile.MentorCategory, new { @class = "form-control", @readonly = "readonly" })
                                                                                    }
                                                                                    
                                                                                    <!--saka-->

                                                                                    <select class="form-control select2 select2-multiple" id="myWillings" multiple="multiple" value=@Model.MyProfile.MentorCategory  name="myWillingToBe" data-placeholder="Select the areas">
                                                                                       
                                                                                            @if (ViewBag.myWillings != null)
                                                                                            {
                                                                                                foreach (var item in ViewBag.myWillings)
                                                                                                {
                                                                                                    if (item.Text != null)
                                                                                                    {
                                                                                                        <option value="@item.Text">
                                                                                                            @item.Text
                                                                                                        </option>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        </select>

                                                                                        <!--sas-->
</div>


                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Sub Specialty</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.Specialty, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Research Interest or Focus</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.Interest, new { @class = "form-control", autofocus = "autofocus", required = true })




                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Number of years in Field of Expertise</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.fieldExpertise, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>


                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Country</label>
                                                                                    @Html.DropDownListFor(m => m.MyProfile.Country, new SelectList(Model.MyProfile.Countries, "Name", "Name"), "Select Country", new { @class = "form-control" })
                                                                                    @Html.ValidationMessageFor(m => m.MyProfile.Country)
                                                                                </div>


                                                                            </div>
                                                                            <div class="form-actions mt-10">
                                                                                <table>
                                                                                    <tr>
                                                                                        <td>
                                                                                            <button type="submit" class="btn btn-success mr-10 mb-30">Update profile</button>
                                                                                        </td>
                                                                                        <td>
                                                                                            &nbsp;&nbsp;
                                                                                        </td>
                                                                                        <td>
                                                                                            <button class="btn fa-refresh"> <a href="@Url.Action("UserProfile", "MentorPartner")"> Refresh</a>
                                                                                            </button>
                                                                                        </td>
                                                                                    </tr>
                                                                                </table>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>

                                <!-- /.modal-dialog -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-9 col-xs-12">
        <div class="panel panel-default card-view pa-0">
            <div class="panel-wrapper collapse in">
                <div class="panel-body pb-0">
                    <div class="tab-struct custom-tab-1">
