                                                <!-- Row -->
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <div class="">
                                                            <div class="panel-wrapper collapse in">
                                                                <div class="panel-body pa-0">
                                                                    <div class="col-sm-12 col-xs-12">
                                                                        <div class="form-wrap">

                                                                            <div class="form-body overflow-hide">
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1"></label>
                                                                                    <div class="input-group">
                                                                                        @if (@Model.MyProfile.ProfilePicsName == string.Empty && Model.MyProfile.Gender == "Male")
                                                                                        {
                                                                                            <img ID="PictureImageA" Height="140" Width="137" src="~/Content/dist/img/userMale.png" />
                                                                                        }

                                                                                        else if (@Model.MyProfile.ProfilePicsName == string.Empty && Model.MyProfile.Gender == "Female")
                                                                                        {
                                                                                            <img ID="PictureImageA" Height="140" Width="137" src="~/Content/dist/img/userfeMale.png" />
                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            <img ID="PictureImageA" Height="140" Width="137" src="~/Files/@Model.MyProfile.ProfilePicsName" />
                                                                                        }



                                                                                        <input type="file" name="postedFile" value=@Model.MyProfile.ProfilePicsName />
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10">Title</label>
                                                                                    @Html.DropDownListFor(m => m.MyProfile.Title, new SelectList(Model.MyProfile.Titles, "Name", "Name"), "Select Title", new { @class = "form-control" })
                                                                                    @Html.ValidationMessageFor(m => m.MyProfile.Title)
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">First Name</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.FName, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Last Name</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.LName, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputuname_1">Degree</label>
                                                                                    <div class="input-group">
                                                                                        <div class="input-group-addon"><i class="icon-user"></i></div>
                                                                                        @Html.TextBoxFor(m => m.MyProfile.Degree, new { @class = "form-control", autofocus = "autofocus", required = true })
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="control-label mb-10" for="exampleInputEmail_1">Email address</label>
